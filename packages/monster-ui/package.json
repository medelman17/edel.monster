{
	"name": "@edelman/monster-ui",
	"version": "0.0.1-alpha.01",
	"type": "module",
	"svelte": "index.js",
	"types": "index.d.ts",
	"author": {
		"name": "Navneet Sharma",
		"url": "https://github.com/navneetsharmaui",
		"email": "navneetsharma.tech@gmail.com"
	},
	"license": "MIT",
	"keywords": [
		"sveltekit",
		"typescript",
		"svelte",
		"SSR",
		"svelte-starter",
		"sveltekit-starter",
		"svelte-boilerplate",
		"sveltekit-boilerplate",
		"svelte boilerplate",
		"sveltekit boilerplate",
		"svelte-tailwind-boilerplate",
		"svelte tailwind boilerplate",
		"sveltekit tailwind boilerplate",
		"svelte-ssr-starter",
		"sveltekit-ssr-starter",
		"svelte-pwa-starter",
		"sveltekit-pwa-starter",
		"starter-template",
		"svelte ssr starter",
		"sveltekit ssr starter",
		"svelte pwa starter",
		"sveltekit pwa starter",
		"starter template",
		"template"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/navneetsharmaui/sveltekit-starter.git"
	},
	"scripts": {
		"prepare": "husky install",
		"prebuild": "rimraf .svelte-kit && rimraf build && rimraf package",
		"pre-commit": "lint-staged",
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .",
		"lint:fix": "prettier --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js}",
		"eslint": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .",
		"eslint:fix": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore . --fix",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"storybook": "start-storybook -p 6006 -s static",
		"build:storybook": "build-storybook -c .storybook -o public -s static",
		"prebuild:storybook": "npm run test:generate-output",
		"build-storybook": "build-storybook",
		"package": "svelte-kit package",
		"test": "jest",
		"test:generate-output": "jest --json --outputFile=.jest-test-results.json"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/plugin-transform-modules-commonjs": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@etchteam/storybook-addon-css-variables-theme": "^1.0.2",
		"@storybook/addon-a11y": "^6.3.12",
		"@storybook/addon-actions": "^6.3.12",
		"@storybook/addon-docs": "^6.3.12",
		"@storybook/addon-essentials": "^6.3.12",
		"@storybook/addon-info": "^5.3.21",
		"@storybook/addon-jest": "^6.3.12",
		"@storybook/addon-links": "^6.3.12",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^1.1.0",
		"@storybook/preset-scss": "^1.0.3",
		"@storybook/svelte": "^6.3.12",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.15.0",
		"@testing-library/svelte": "^3.0.3",
		"@types/cookie": "^0.4.1",
		"@types/jest": "^27.0.2",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@washingtonpost/storybook-addon-web-vitals": "^0.1.1",
		"autoprefixer": "^10.4.0",
		"babel-jest": "^27.3.1",
		"babel-loader": "^8.2.3",
		"concurrently": "^6.4.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.8",
		"cssnano-preset-advanced": "^5.1.7",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"gray-matter": "^4.0.3",
		"husky": "^7.0.4",
		"jest": "^27.3.1",
		"lint-staged": "^12.0.3",
		"postcss": "^8.3.11",
		"postcss-cli": "^9.0.2",
		"postcss-import": "^14.0.2",
		"postcss-load-config": "^3.1.0",
		"postcss-nesting": "^10.0.1",
		"postcss-scss": "^4.0.2",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rimraf": "^3.0.2",
		"sass": "^1.43.4",
		"sass-loader": "^12.3.0",
		"stylelint": "^14.1.0",
		"stylelint-config-recommended": "^6.0.0",
		"stylelint-config-standard": "^24.0.0",
		"svelte": "^3.44.1",
		"svelte-check": "^2.2.6",
		"svelte-jester": "^2.1.5",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.9.8",
		"svelte2tsx": "^0.4.10",
		"ts-jest": "^27.0.7",
		"tslib": "^2.3.1",
		"typescript": "^4.4.4",
		"vite-imagetools": "^4.0.0"
	},
	"jest": {
		"collectCoverage": false,
		"collectCoverageFrom": [
			"src/lib/**/*.ts",
			"src/lib/**/*.svelte",
			"!src/**/routes/**",
			"!**/node_modules/**",
			"!**/partials/**",
			"!**/fixture/**"
		],
		"coveragePathIgnorePatterns": [
			"node_modules",
			"fixtures"
		],
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"transform": {
			"^.+\\.svelte$": [
				"svelte-jester",
				{
					"preprocess": true
				}
			],
			"^.+\\.ts$": "ts-jest",
			"^.+\\.js$": "babel-jest",
			"^.+\\.mdx$": "@storybook/addon-docs/jest-transform-mdx"
		},
		"moduleFileExtensions": [
			"js",
			"ts",
			"svelte",
			"mdx"
		]
	}
}
